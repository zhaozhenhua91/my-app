page{background-color:#fff}.feedback-container{padding:10px}.feedback-container .feedback-title{font-size:16px;color:#999}.feedback-container .feedback-content{border:1px solid #f5f5f5;padding:10px;margin-top:20px}.feedback-container .feedback-image-title{font-size:16px;color:#999;margin-top:20px}.feedback-container .feedback-image-box{margin-top:20px;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-flex-wrap:wrap;flex-wrap:wrap}.feedback-container .feedback-image-box .feedback-image-item{position:relative;width:33.3%;height:0;padding-top:33.3%}.feedback-container .feedback-image-box .feedback-image-item .close-icon{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;position:absolute;top:0;right:0;z-index:2;background-color:#f07373;width:22px;height:22px;-webkit-border-radius:11px;border-radius:11px}.feedback-container .feedback-image-box .feedback-image-item .image-box{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;position:absolute;top:5px;right:5px;bottom:5px;left:5px;border:1px solid #eee;-webkit-border-radius:5px;border-radius:5px;overflow:hidden}.feedback-container .feedback-image-box .feedback-image-item .image-box image{width:100%;height:100%}.feedback-container .feedback-btn{margin:5px 10px;background-color:#f07373}